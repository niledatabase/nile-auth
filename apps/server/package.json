{
  "name": "@nile-auth/server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--trace-warnings' next dev -p 3001",
    "build": "next build",
    "build:spec": "yarn next-swagger-doc-cli openapi/next-swagger-doc.json && mv public/swagger.json openapi",
    "build:client": "yarn openapi-generator-cli generate -i openapi/swagger.json -g typescript-fetch -o openapi/region-rest --additional-properties=ngVersion=6.1.7,supportsES6=true,npmVersion=6.9.0,withInterfaces=true,typescriptThreePlus=true",
    "start": "next start -p 3001",
    "lint": "next lint",
    "test": "jest"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^15.0.1",
    "@nile-auth/core": "*",
    "@nile-auth/logger": "*",
    "@nile-auth/query": "*",
    "@panva/hkdf": "^1.2.1",
    "@simplewebauthn/browser": "^11.0.0",
    "@simplewebauthn/server": "^11.0.0",
    "bcryptjs": "^2.4.3",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.5",
    "next": "^14.2.21",
    "nodemailer": "^6.9.15",
    "oauth": "^0.10.0",
    "oauth4webapi": "^3.1.0",
    "openid-client": "^5.7.0",
    "pg": "^8.13.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swagger-ui-react": "^5.21.0",
    "uuid": "^10.0.0",
    "winston": "^3.14.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@nile-auth/eslint-config": "workspace:*",
    "@nile-auth/typescript-config": "workspace:*",
    "@niledatabase/server": "latest",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/bcryptjs": "^2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6",
    "@types/oauth": "^0",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/swagger-ui-react": "^4.18.3",
    "@types/uuid": "^10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-swagger-doc": "^0.4.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5"
  }
}
